                            <p>
                                <label for="snackDescription">Description</label>
                                <input id="snackDescription" type="text" name="description" value="">
                            </p>
                            <p>
                                <label for="imgUrl">Image URL:</label>
                                <input id="imgUrl" type="text" name="imgUrl" value="">
                            </p>
                            <input type="submit" value="add snack">
                        </form>
					</p>
				</div>
				
			</div>
		</div>
		<div class="col-md-4">
		</div>
	</div>
</div>

