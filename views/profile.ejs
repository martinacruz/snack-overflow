<a href="/auth/logout">Logout</a> |
<a href="/profile">Profile</a>
<h2>Snacks</h2>
<h3>Welcome to your Snack Profile</h3>

<p>See Your Snacks (DB pull list goes here)</p>

<% snacks.forEach((snack) => { %>
      <h4 id = "snackName"><%= snack.name %></h4>
      <p><%=snack.description%></p>
      <img height="200px" width="200px" src='<%=snack.imgUrl%>'>
      <a href="/detail/ <%= snack.id  %> ">View Snack</a>
      <form method="POST" action="/profile/<%= snack.id %>?_method=DELETE%>">
        <input type="hidden" value="Delete" name="name"> 
        <button type="submit">Blechk</button>
      </form>
      <!-- allow as user to add an image if they want to change it -->
      <form method="POST" action="/edit/<%=snack.id%>">
        <input id="newImage" type="text" name="imgUrl"value="janky Url here">
        <button type="submit">update image</button>
      </form>
      
  <% }); %>
<a href="/snack"> <h1>Add a Snack</h1></a>
